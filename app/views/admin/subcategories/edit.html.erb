<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'Список', admin_subcategories_path  %></li>
      <li class="active"><%= link_to 'Редактировать', edit_admin_subcategory_path(@subcategory.id) %></li>
    </ul>
    <div class="clear"></div>
  </div>
  <div class="content">
    <h2 class="title">Редактирование подкатегории</h2>
    <div class="inner">
      <% form_for [:admin, @subcategory], :method => :put, :html => { :multipart => true, :class => :form }  do |form| %>
        <%= error_messages_for :subcategory %>
        <%= render :partial => "form", :locals => {:form => form} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>