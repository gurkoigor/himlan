<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', admin_subcategories_path  %></li>
    </ul>
    <div class="clear"></div>
  </div>
  <div class="content">
    <h2 class="title">Создание подкатегории</h2>
    <div class="inner">
      <% form_for [:admin, @subcategory], :html => { :multipart => true, :class => :form }  do |form| %>
        <%= error_messages_for :subcategory %>
        <%= render :partial => "form", :locals => {:form => form} %>
        <div class="group navform">
          <%= form.submit "Создать", :class => "button" %> or <%= link_to "Назад", admin_subcategories_path %>
        </div>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>