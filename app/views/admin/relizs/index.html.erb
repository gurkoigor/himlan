<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first active">  <%= link_to "Добавить", new_admin_reliz_path %></li>
    </ul>
    <div class="clear"></div>
  </div>
  <div class="content">
    <h2 class="title">Релизы</h2>
    <div class="inner">
      <table class="table">
        <th class="first">Категория</th>
        <th>Подкатегория</th>
        <th>Название</th>
        <th>Добавлен</th>
        <th>Обновлен</th>
        <th>Действия</th>
        <% unless @relizs.nil? %>
          <%= render :partial => 'reliz', :collection => @relizs %>
        <% end %>
      </table>
      <div class="actions-bar">
        <div class="actions">
        </div>

        <%= will_paginate @relizs  %>

        <div class="clear"></div>
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>